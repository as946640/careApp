page {
  background-color: #EEEEEE;
}
.pages {
  font-size: 11pt;
  width: 94%;
  margin: 20rpx auto;
  padding-bottom: 15%;
}
.pages .send {
  font-size: 12pt;
  color: #fff;
  background-size: 100%;
  background-image: url(http://gc.sinbel.cn/web/statics//wxapp/images/img-order-status-bar.png);
  padding: 50rpx;
  border-radius: 15rpx;
}
.pages .send .left {
  width: 60%;
}
.pages .send .left image {
  width: 100rpx;
  height: 100rpx;
  display: block;
  margin-left: auto;
}
.pages .expressage {
  line-height: 50rpx;
  padding: 20rpx;
  position: relative;
  background-color: #FFFFFF;
  margin: 20rpx auto;
  border-radius: 15rpx;
}
.pages .expressage .right {
  width: 15%;
  padding: 10rpx;
  box-sizing: border-box;
}
.pages .expressage .right image {
  width: 100%;
  height: 100%;
}
.pages .expressage .go_right {
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 20rpx;
}
.pages .user {
  padding: 20rpx;
  background-color: #FFFFFF;
  margin: 20rpx 0;
  border-radius: 15rpx;
}
.pages .user .right {
  width: 15%;
  padding: 5px;
  box-sizing: border-box;
}
.pages .user .right image {
  width: 100%;
  height: 100%;
}
.pages .user .user_linkman view {
  width: 100%;
}
.pages .user .user_linkman view:last-child {
  text-align: right;
}
.pages .order {
  padding: 20rpx;
  height: 100%;
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  border-radius: 15rpx;
  background-color: #fff;
}
.pages .order .order_info {
  width: 100%;
  font-size: 10pt;
  background-color: #FFFFFF;
}
.pages .order .order_info .left {
  width: 40%;
  min-width: 40%;
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
  border-top-left-radius: 24rpx;
  border-bottom-left-radius: 24rpx;
}
.pages .order .order_info .left image {
  width: 100%;
  height: 100%;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}
.pages .order .order_info .right {
  width: 60%;
  min-width: 60%;
  font-size: 10pt;
  padding: 10rpx;
  box-sizing: border-box;
}
.pages .order .order_info .right .goods_title {
  width: 90%;
  font-size: 11pt;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.pages .order .order_info .right .attr {
  color: #BCBCBC;
  width: 80%;
  font-size: 10pt;
}
.pages .order .order_info .right .price {
  margin-top: 15%;
  font-size: 11pt;
  text-align: right;
}
.pages .order .order_info .bottom {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 10rpx;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
}
.pages .order .order_info .bottom .btnitem {
  width: 50%;
  height: 50rpx;
  font-size: 9pt;
  color: #555555;
  margin-right: 5rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 20rpx;
}
.pages .goods {
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #FFFFFF;
  margin: 20rpx 0;
  border-radius: 15rpx;
}
.pages .goods .order_list .list view {
  width: 50%;
  margin-top: 5rpx;
}
.pages .goods .order_list .list .left {
  width: 100%;
  text-align: left !important;
}
.pages .goods .order_list .list view:last-child {
  text-align: right;
}
.pages .goods .sum {
  text-align: right;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1px solid #EEEEEE;
}
.pages .btn {
  width: 94%;
  position: fixed;
  box-sizing: border-box;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  bottom: 20rpx;
  background-color: #F15E00;
  border-radius: 20rpx;
}
.pages .btn .btnitem {
  width: 100%;
  height: 80rpx;
  font-size: 10pt;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  border-radius: 20rpx;
}

