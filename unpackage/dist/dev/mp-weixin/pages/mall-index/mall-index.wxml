ck></block><block wx:if="{{module.name=='notice'}}"><block><view class="banner"><image src="{{active_list}}"></image></view></block></block><block wx:if="{{module.name=='miaosha'}}"><block><view class="promotion"><view class="text">优惠专区</view><view class="list"><block wx:for="{{Promotion}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toPromotion',['$0'],[[['Promotion','',index]]]]]]]}}" class="column" bindtap="__e"><view class="top"><view class="title">{{row.title}}</view><block wx:if="{{row.countdown}}"><view class="countdown"><view>{{row.countdown.h}}</view>:<view>{{row.countdown.m}}</view>:<view>{{row.countdown.s}}</view></view></block></view><view class="left"><view class="ad">{{row.ad}}</view><view class="into">点击进入</view></view><view class="right"><image src="{{row.img}}"></image></view></view></block></view></view></block></block><block wx:if="{{module.name=='cat'}}"><block><view class="goods-list"><view class="title"><scroll-view class="tui-scroll-h" id="tab-bar" scroll-with-animation="{{true}}" scroll-x="{{true}}" show-scrollbar="{{false}}" scroll-into-view="{{scrollInto}}"><block wx:for="{{tabBars}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="tui-tab-item" id="{{tab.name}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tabClick',['$event']]]]]}}" bindtap="__e"><view class="{{['tui-tab-item-title',(tabIndex==index)?'tui-tab-item-title-active':'']}}">{{tab.name}}</view></view></block></scroll-view></view><block wx:if="{{tabIndex==tabIndex}}"><block><view class="product-list"><block wx:for="{{tabBars[tabIndex].goods_list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['tabBars.'+tabIndex+'.goods_list','id',item.id]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{item.pic_url}}"></image><view class="name">{{item.name}}</view><view class="info"><view class="price">{{item.price}}</view><view class="slogan"