<view><view class="tabr" style="{{'top:'+(headerTop)+';'}}"><view data-event-opts="{{[['tap',[['switchType',['valid',0]]]]]}}" class="{{[(typeClass=='valid')?'on':'']}}" bindtap="__e">可用</view><view data-event-opts="{{[['tap',[['switchType',['invalid',1]]]]]}}" class="{{[(typeClass=='invalid')?'on':'']}}" bindtap="__e">已使用</view><view class="{{['border',typeClass]}}"></view></view><view class="place"></view><view class="list"><view class="sub-list valid"><block wx:for="{{couponValidList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view class="row"><view class="carrier"><view class="left"><view class="title">{{''+row.event_desc+''}}</view><view class="term">{{''+row.begin_time+" ~ "+row.end_time+''}}<block wx:if="{{row.cat.length==0}}"><block><view>全场通用</view></block></block><block wx:for="{{row.cat}}" wx:for-item="cat" wx:for-index="index"><block wx:if="{{row.cat.length!=0}}"><block><view>仅限<text>{{cat.name}}</text>产品使用</view></block></block></block></view><view class="gap-top"></view><view class="gap-bottom"></view></view><view class="right"><view class="ticket"><view class="num">{{''+row.sub_price+'元'}}</view></view><view class="criteria">{{'满'+row.min_price+'使用'}}</view><block wx:if="{{row.is_use==1}}"><view class="criteria">已使用</view></block></view></view></view></block></view></view></view>